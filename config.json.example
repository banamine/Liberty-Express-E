{
  "api": {
    "port": 3000,
    "host": "0.0.0.0",
    "cors": {
      "enabled": true,
      "origin": "*"
    },
    "timeout": 30000
  },
  "python": {
    "executable": "python3",
    "maxConcurrentProcesses": 4,
    "timeout": 30000,
    "maxQueueSize": 1000
  },
  "paths": {
    "output": "./api_output",
    "schedules": "./api_output/schedules",
    "exports": "./api_output/exports",
    "logs": "./logs"
  },
  "logging": {
    "level": "info",
    "format": "json",
    "file": "./logs/scheduleflow.log"
  },
  "features": {
    "autoFill": true,
    "cooldownEnabled": true,
    "cooldownHours": 48,
    "duplicateDetection": true,
    "conflictDetection": true
  },
  "production": {
    "enabled": false,
    "database": {
      "host": "localhost",
      "port": 5432,
      "name": "scheduleflow",
      "user": "scheduleflow",
      "password": "changeme"
    },
    "redis": {
      "enabled": false,
      "host": "localhost",
      "port": 6379
    }
  }
}
