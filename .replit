modules = ["nodejs-20", "web", "python-3.11"]
[agent]
expertMode = true
integrations = ["github:1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["ffmpeg", "freetype", "lcms2", "libGL", "libGLU", "libimagequant", "libjpeg", "libtiff", "libvlc", "libwebp", "libxcrypt", "openjpeg", "tcl", "tk", "zlib"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "ScheduleFlow API Server"

[[workflows.workflow]]
name = "ScheduleFlow API Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "node api_server.js"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 43383
externalPort = 3000

[deployment]
deploymentTarget = "autoscale"
run = ["npx", "serve", "-l", "5000", "--no-clipboard"]

[objectStorage]
defaultBucketID = "replit-objstore-858159f4-0363-4b23-98ee-27b64b61d133"
